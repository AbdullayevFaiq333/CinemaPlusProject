
@{
    ViewData["Title"] = "Index";
    List<DolbyAtmos> dolbyAtmos = Model;
}

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Dolby Atmos</h4>
                <a asp-action="Create" class="btn btn-primary">Create</a>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                @*<th>Logo :  </th> *@
                                <th>FirstTitle : </th>
                                <th>FirstDescription : </th>
                                <th>YoutubeUrl : </th>
                                <th>SecondTitle : </th>
                                <th>SecondDescription : </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (DolbyAtmos dolbyatmos in dolbyAtmos)
                            {
                            <tr>
                                @*<td>
                                    <img src="~/images/@dolbyatmos.Logo" style="width:100px !important ; height:100px !important" />
                                </td>*@
                                <td>
                                    @dolbyatmos.FirstTitle
                                </td>
                                <td>
                                    @dolbyatmos.FirstDescription
                                </td>
                                <td>
                                    @dolbyatmos.YoutubeURL
                                </td>
                                <td>
                                    @dolbyatmos.SecondTitle
                                </td>
                                <td>
                                    @dolbyatmos.SecondDescription
                                </td>


                                <td>
                                    <a asp-action="Detail" asp-route-id="@dolbyatmos.Id" class="btn btn-success">Detail</a>
                                    <a asp-action="Update" asp-route-id="@dolbyatmos.Id" class="btn btn-warning">Update</a>
                                    <a asp-action="Delete" asp-route-id="@dolbyatmos.Id" class="btn btn-danger">Delete</a>
                                </td>

                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>


